.row.winner[prize_winner]
  .large-4.medium-5.small-5.columns
    -if prize_winner.prize.avatar_file_name.present?
      =image_tag("#{prize_winner.prize.avatar.url(:square)}")
    -else
      =image_tag("#{prize_winner.prize.title.gsub(' ', '-')}.png")
  .large-8.medium-7.small-7.columns
    %span=prize_winner.name
    %label won a #{prize_winner.prize.description} from
    %span=prize_winner.prize.title